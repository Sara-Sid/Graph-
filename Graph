import java.util.Arrays;

class Graph{
  int n;
  int[][]edges;
  String node;
  boolean directed;
  int [] subset;

// Constructor â€” runs when you make a new Graph object
  public Graph(int n, boolean directed){
    this.n =n;
    this.directed= directed;
    this.edges = new int[n][n];
  }
    
    public void addNode(String node){
      this.node = node;
    }
   
  
  public void addEdge(int i, int j){
    this.edges[i][j]=1;
    if(!this.directed){
      edges[j][i]=1;
    }
  }
  void printGraph(){
    for (int i=0; i<n; i++){
      for (int j=0; j<n; j++){
        this.edges[i][j]=0;
      }
      System.out.println();
    }
  }
    Graph createSubGraph(Graph myGraph, int[]subset){
      int m = subset.length;
      Graph subsetGraph = new Graph(m, myGraph.directed);
      for (int i=0; i<m; i++){
        for(int j=0; j<m; j++){
          subsetGraph.edges[i][j] = myGraph.edges [subset[i]subset[j]]
        }
      }
      return subsetGraph;
    }
}
